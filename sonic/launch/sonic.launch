<launch>
  <!-- Ultrasonic node -->
  <node name="sonic_read" pkg="sonic" type="sonic_read" output="screen" />

<node name="ultrasonic" pkg="rosserial_python" type="serial_node.py" output="screen">
	<param name="port" value="/dev/ttyUSB0" />
	<param name="baud" value="57600" />
</node>

